//Product
//Attributes
-name:String
-storeId:String
-price:Number
-image:String
-categoryId:Category
-productType:String
-description:String
-offer: Offer
-isInStock:type:Boolean
-tags:String[]
//Methods
+exists(productId):Product
+countAll(type):Int
+countByStore(storeId,type):Int
+countByCategory(categoryId,type):Int
+countByOffers():Int
+countByOffersOfStore(storeId):Int
+validateProductInfo(productInfo):String
+createProduct(productInfo):Product
+updateProduct(updatedProduct):Product
+deleteProduct(productId):Product
+deleteProductsOfCategory(categoryId):Product[]
+getProductById(productId):Product[]
+getProductsOfCategory(categoryId,type,limit,skip,nameSort,priceSort):Product[]
+deleteProductsOfCategoriesId(categoriesIds):Product[]
+addOffer(productId, offer):Product[]
+removeOffer(offerId):Product
+getProductsWithOffers(limit, skip):Product[]
+getProductsWithOffersOfStore(storeId, limit, skip):Product[]
+getAllProducts(type,limit,skip,nameSort,priceSort):Product[]
+getProductsOfStore(storeId,type,limit,skip,nameSort,priceSort):Product[]
+expiredOffers():Product[]
+searchProducts(searchText, limit, skip):Product[]
+findProductAndItsOffer(ProductId):Product[]
